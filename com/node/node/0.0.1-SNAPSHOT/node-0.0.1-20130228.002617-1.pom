	<properties>

		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- JBoss -->
		<jboss.bom.version>1.0.2.Final</jboss.bom.version>
		<seam.version>3.1.0.Final</seam.version>
		<infinispan.version>5.1.6.FINAL</infinispan.version>
		<weld-api.version>1.1.Final</weld-api.version>
		<weld-core.version>1.1.5.AS71.Final</weld-core.version>



		<!-- JSF -->
		<jsf-api.version>2.1</jsf-api.version>
		<jsf-impl.version>2.1.7</jsf-impl.version>
		<prime.version>3.5</prime.version>
		<prime-ext.version>0.6.3</prime-ext.version>
		<prettyfaces.version>3.3.3</prettyfaces.version>
		<omnifaces.version>1.3</omnifaces.version>
		<javax.el-api.version>2.2.4</javax.el-api.version>

		<!-- Apache commons -->
		<apache.commons-lang3.version>3.1</apache.commons-lang3.version>
		<apache-commons-dbutils.version>1.5</apache-commons-dbutils.version>
		<apache-commons-collections.version>3.2.1</apache-commons-collections.version>

		<!-- Other -->
		<uuid.version>3.2</uuid.version>


		<internal.repo.path>file://C/Users/Anatolian/git/mvn-repo/</internal.repo.path>



	</properties>

	<repositories>

		<repository>
			<id>prime-repo</id>
			<name>PrimeFaces Maven Repository</name>
			<url>http://repository.primefaces.org</url>
			<layout>default</layout>
		</repository>

		<repository>
			<id>eaio.com</id>
			<url>http://eaio.com/maven2</url>
		</repository>

	</repositories>

	<dependencyManagement>
		<dependencies>

			<dependency>
				<groupId>org.jboss.bom</groupId>
				<artifactId>jboss-javaee-6.0-with-tools</artifactId>
				<version>${jboss.bom.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>

			<dependency>
				<groupId>org.jboss.bom</groupId>
				<artifactId>jboss-javaee-6.0-with-hibernate</artifactId>
				<version>${jboss.bom.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>

			<!-- <dependency> <groupId>org.jboss.bom</groupId> <artifactId>jboss-javaee-6.0-with-infinispan</artifactId> 
				<version>${jboss.bom.version}</version> <type>pom</type> <scope>import</scope> 
				</dependency> -->

			<dependency>
				<groupId>org.jboss.bom</groupId>
				<artifactId>jboss-javaee-6.0-with-transactions</artifactId>
				<version>${jboss.bom.version}</version>
